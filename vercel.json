{
    "builds": [{
        "src": "ssocial/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" },
        "dependencies": ["requirements.txt"]
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "ssocial/wsgi.py"
        }
    ]
}